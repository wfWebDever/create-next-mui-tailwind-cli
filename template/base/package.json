{
  "name": "my-create-next-mui-tailwind-project",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
